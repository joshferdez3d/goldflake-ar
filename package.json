{
  "name": "ar-web-application",
  "version": "1.0.0",
  "description": "AR Web Application with user authentication flow and 8th Wall integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run lint && npm test",
    "docker:build": "docker build -t ar-web-app .",
    "docker:run": "docker run -p 3000:3000 ar-web-app",
    "deploy": "npm run build && npm run docker:build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ar-web-application.git"
  },
  "keywords": [
    "ar",
    "augmented-reality",
    "8th-wall",
    "web-app",
    "nodejs",
    "express",
    "mobile-first"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/ar-web-application/issues"
  },
  "homepage": "https://github.com/yourusername/ar-web-application#readme"
}
